{
	"name": "Preact-Flux-Bootstrap",
	"version": "1.0.0",
	"description": "Create a small project that can be easily added to any UI.",
	"main": "index.js",
	"scripts": {
		"build": "browserify index.js -o bundle.js -t [ babelify ] ",
		"test": "echo \"Error: no test specified\" && exit 1",
		"watch": "watchify index.js -o bundle.js -t [ babelify ] -v",
