	"license": "ISC",
	"devDependencies": {
		"babel-preset-es2015": "^6.6.0",
		"babel-preset-react": "^6.5.0",
		"babelify": "^7.2.0",
		"browserify": "^13.0.0",
		"concurrently": "^2.0.0",
		"watchify": "^3.7.0"
	},
	"dependencies": {
		"axios": "^0.10.0",
		"flux": "^2.1.1",
		"object-assign": "^4.0.1",
		"preact": "^4.7.2"
	}
